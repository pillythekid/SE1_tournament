<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/RegisterNewPlayerRequest"
	xmlns:tns="http://www.example.org/RegisterNewPlayerRequest"
	elementFormDefault="qualified">
	<element name="name">
		<simpleType>
			<restriction base="string">
				<minLength value="3"></minLength>
				<maxLength value="40"></maxLength>
			</restriction>
		</simpleType>
	</element>
	<element name="mtrnr">
		<simpleType>
			<restriction base="string">
				<minLength value="7"></minLength>
				<maxLength value="8"></maxLength>
			</restriction>
		</simpleType>
	</element>
</schema>